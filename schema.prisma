generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model todos {
  todoId    String      @id @unique @default(uuid())
  name String  
  description  String
  userId String
 
}

model users {
  id        String      @id @unique @default(uuid())
  name     String 
  email   String  @unique
  password  String
}